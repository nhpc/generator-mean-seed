{
	"name": "<%= optAppName %>",
	"version": "0.0.0",
	"homepage": "https://github.com/<%= optGithubName%>/<%= optAppName %>",
	"authors": [
		"<%= optAuthorName %>"
	],
	"description": "<%= optAppDescription %>",
	"keywords": [
		<% for(var ii=0; ii<optAppKeywords.length; ii++) {
			if(ii !==0) {
				print('\t\t');
			}
			print('"'+optAppKeywords[ii]+'"');
			if(ii !==(optAppKeywords.length -1)) {
				print(',\n');
			}
		}
		%>
	],
	"dependencies": {
		"angular":"1.2.5",
		"angular-animate":"1.2.5",
		"angular-sanitize":"1.2.5",
		"angular-touch":"1.2.5",
		"angular-route":"1.2.5",
		"angular-cookies":"1.2.5",
		"bourbon":"latest",
		"font-awesome":"latest",
		"less-flexbox":"latest",
		"momentjs":"latest",
		"hammerjs":"latest",
		"angular-hammer":"latest",
		"angular-string":"latest",
		"angular-array":"latest",
		"angular-facebook-auth":"latest",
		"angular-google-auth":"latest",
		"angular-forminput":"latest"
	},
	"license": "MIT",
	"resolutions": {
		"angular": "1.2.5",
		"angular-animate":"1.2.5",
		"angular-sanitize":"1.2.5",
		"angular-touch":"1.2.5",
		"angular-route":"1.2.5",
		"angular-cookies":"1.2.5"
	}
}
